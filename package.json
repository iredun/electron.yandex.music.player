{
  "name": "electron-yandex-music-palyer",
  "version": "1.0.0",
  "description": "A player for Yandex.Music, based web browser",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . ya.music.player --out=builds --ignore=node-modules/electron-* --icon=./images/icon.ico --overwrite"
  },
  "repository": "https://github.com/iredun/electron.yandex.music.player.git",
  "keywords": [
    "Electron",
    "Yandex",
    "Music",
    "Player"
  ],
  "build": {
    "appId": "yandex.music.player"
  },
  "author": "IvanRedun",
  "devDependencies": {
    "electron": "^2.0.8",
    "electron-packager": "^12.1.1"
  },
  "dependencies": {
    "electron-native-notification": "^1.2.1",
    "node-notifier": "^5.2.1",
    "npm": "^6.4.1"
  }
}
